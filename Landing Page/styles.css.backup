  backdrop-filter: blur(10px);
  box-shadow:
    0 8px 32px rgba(0, 0, 0, 0.3),
    inset 0 1px 0 rgba(255, 255, 255, 0.1);
}

.game-thumb:hover {
  opacity: 1;
  transform: scale(1.05) !important;
  border-color: var(--accent);
  box-shadow:
    0 12px 40px rgba(179, 255, 87, 0.3),
    inset 0 1px 0 rgba(255, 255, 255, 0.2);
}

.thumb-placeholder {
  font-size: 48px;
  filter: drop-shadow(0 2px 8px rgba(0, 0, 0, 0.3));
}

/* Individual thumbnail positions and animations */
.game-thumb-1 {
  top: 10%;
  left: 8%;
  animation-delay: 0s;
  animation-duration: 25s;
}

.game-thumb-2 {
  top: 15%;
  right: 12%;
  animation-delay: -3s;
  animation-duration: 22s;
}

.game-thumb-3 {
  top: 50%;
  left: 5%;
  animation-delay: -6s;
  animation-duration: 28s;
}

.game-thumb-4 {
  top: 55%;
  right: 8%;
  animation-delay: -9s;
  animation-duration: 24s;
}

.game-thumb-5 {
  bottom: 15%;
  left: 15%;
  animation-delay: -12s;
  animation-duration: 26s;
}

.game-thumb-6 {
  bottom: 20%;
  right: 18%;
  animation-delay: -15s;
  animation-duration: 23s;
}

.game-thumb-7 {
  top: 30%;
  left: 12%;
  animation-delay: -18s;
  animation-duration: 27s;
  width: 100px;
  height: 100px;
}

.game-thumb-8 {
  top: 35%;
  right: 15%;
  animation-delay: -21s;
  animation-duration: 25s;
  width: 100px;
  height: 100px;
}

@keyframes drift {
  from {
    transform: translate(0, 0) scale(1);
  }
  to {
    transform: translate(30px, -30px) scale(1.05);
  }
}

@keyframes float {
  0%,
  100% {
    transform: translateY(0) rotate(0deg);
  }
  25% {
    transform: translateY(-20px) rotate(2deg);
  }
  50% {
    transform: translateY(0) rotate(0deg);
  }
  75% {
    transform: translateY(20px) rotate(-2deg);
  }
}

.hero-eyebrow {
  font-family: var(--mono);
  font-size: 11px;
  letter-spacing: 0.2em;
  text-transform: uppercase;
  color: var(--accent);
  margin-bottom: 24px;
  display: flex;
  align-items: center;
  gap: 12px;
  animation: fadeUp 0.6s ease both;
}

.hero-eyebrow::before {
  content: "";
  display: block;
  width: 32px;
  height: 1px;
  background: var(--accent);
  box-shadow: 0 0 8px var(--accent);
}

.hero h1 {
  font-family: var(--display);
  font-size: clamp(80px, 12vw, 180px);
  line-height: 0.9;
  letter-spacing: 0.03em;
  color: var(--text);
  max-width: 900px;
  animation: fadeUp 0.8s ease both;
  position: relative;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
}

.hero h1 .outline {
  -webkit-text-stroke: 2px var(--text);
  color: transparent;
  position: relative;
  display: inline-block;
  transition: var(--transition-smooth);
}

.hero h1 .outline:hover {
  transform: scale(1.02);
  filter: drop-shadow(0 0 20px rgba(179, 255, 87, 0.3));
}

.hero h1 .glow {
  color: var(--accent);
  text-shadow:
    0 0 60px var(--glow-pink),
    0 0 120px var(--glow-pink);
  position: relative;
  display: inline-block;
  transition: var(--transition-smooth);
}

.hero h1 .glow:hover {
  transform: scale(1.02);
  text-shadow: 
    0 0 30px var(--glow-cyan),
    0 0 60px var(--glow-cyan);
}

.hero-sub {
  margin-top: 36px;
  font-size: 17px;
  color: var(--muted);
  max-width: 480px;
  line-height: 1.7;
  animation: fadeUp 0.6s ease 0.2s both;
}

.hero-sub strong {
  color: var(--text);
  font-weight: 500;
}

.hero-actions {
  margin-top: 48px;
  display: flex;
  align-items: center;
  gap: 24px;
  animation: fadeUp 0.6s ease 0.3s both;
}

.btn-primary {
  font-family: var(--mono);
  font-size: 12px;
  letter-spacing: 0.12em;
  text-transform: uppercase;
  color: var(--bg);
  background: var(--accent);
  padding: 18px 40px;
  border-radius: 2px;
  text-decoration: none;
  display: inline-flex;
  align-items: center;
  gap: 10px;
  transition: var(--transition-smooth);
  position: relative;
  overflow: hidden;
  box-shadow: 0 4px 16px rgba(179, 255, 87, 0.3);
}

.btn-primary::after {
  content: "";
  position: absolute;
  inset: 0;
  background: linear-gradient(
    135deg,
    rgba(255, 255, 255, 0.2) 0%,
    transparent 60%
  );
}

.btn-primary::before {
  content: "";
  position: absolute;
  top: 50%;
  left: 50%;
  width: 0;
  height: 0;
  border-radius: 50%;
  background: rgba(255, 255, 255, 0.3);
  transform: translate(-50%, -50%);
  transition: width 0.6s, height 0.6s;
}

.btn-primary:hover {
  box-shadow:
    var(--glow-accent-strong),
    0 8px 24px rgba(179, 255, 87, 0.4);
  transform: translateY(-3px) scale(1.02);
}

.btn-primary:hover::before {
  width: 300px;
  height: 300px;
}

.btn-primary:active {
  transform: translateY(-1px) scale(0.98);
}

.btn-primary svg {
  transition: transform 0.3s;
}

.btn-primary:hover svg {
  transform: translateX(4px);
}

.btn-secondary {
  font-family: var(--mono);
  font-size: 12px;
  letter-spacing: 0.12em;
  text-transform: uppercase;
  color: var(--muted);
  text-decoration: none;
  display: inline-flex;
  align-items: center;
  gap: 8px;
  transition: var(--transition-smooth);
  padding: 8px 16px;
  border-radius: 2px;
  border: 1px solid rgba(179, 255, 87, 0.2);
  position: relative;
  overflow: hidden;
}

.btn-secondary::before {
  content: "";
  position: absolute;
  inset: 0;
  background: linear-gradient(135deg, rgba(179, 255, 87, 0.1), transparent);
  opacity: 0;
  transition: opacity 0.3s;
}

.btn-secondary:hover {
  color: var(--accent);
  border-color: var(--accent);
  box-shadow: 0 0 20px rgba(179, 255, 87, 0.2);
}

.btn-secondary:hover::before {
  opacity: 1;
}

.btn-secondary svg {
  transition: transform 0.3s;
}

.btn-secondary:hover svg {
  transform: translateY(3px);
}

/* Scroll indicator */
.scroll-hint {
  position: absolute;
  bottom: 40px;
  left: 56px;
  display: flex;
  align-items: center;
  gap: 12px;
  font-family: var(--mono);
  font-size: 10px;
  letter-spacing: 0.12em;
  text-transform: uppercase;
  color: rgba(245, 244, 255, 0.25);
  animation: fadeUp 0.6s ease 0.5s both;
}

.scroll-line {
  width: 1px;
  height: 48px;
  background: linear-gradient(to bottom, var(--accent), transparent);
  animation: scrollPulse 2s ease-in-out infinite;
}

@keyframes scrollPulse {
  0%,
  100% {
    opacity: 0.3;
  }
  50% {
    opacity: 1;
  }
}

/* ── MARQUEE ── */
.marquee-wrap {
  border-top: 1px solid var(--border);
  border-bottom: 1px solid var(--border);
  overflow: hidden;
  padding: 18px 0;
  background: var(--surface);
  position: relative;
}

.marquee-wrap::before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  width: 100px;
  background: linear-gradient(90deg, var(--surface), transparent);
  z-index: 2;
  pointer-events: none;
}

.marquee-wrap::after {
  content: "";
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  width: 100px;
  background: linear-gradient(-90deg, var(--surface), transparent);
  z-index: 2;
  pointer-events: none;
}

.marquee-track {
  display: flex;
  gap: 0;
  animation: marquee 30s linear infinite;
  white-space: nowrap;
}

.marquee-track:hover {
  animation-play-state: paused;
}

.marquee-item {
  font-family: var(--display);
  font-size: 15px;
  letter-spacing: 0.2em;
  color: var(--muted);
  padding: 0 32px;
  display: flex;
  align-items: center;
  gap: 32px;
  transition: color 0.3s;
}

.marquee-item:hover {
  color: var(--accent);
}

.marquee-item .dot {
  width: 5px;
  height: 5px;
  border-radius: 50%;
  background: var(--accent);
  box-shadow: 0 0 12px var(--accent);
  animation: pulse-dot 2s ease-in-out infinite;
}

@keyframes pulse-dot {
  0%, 100% {
    transform: scale(1);
    opacity: 1;
  }
  50% {
    transform: scale(1.2);
    opacity: 0.7;
  }
}

@keyframes marquee {
  from {
    transform: translateX(0);
  }
  to {
    transform: translateX(-50%);
  }
}

/* ── WHAT WE DO ── */
.section {
  padding: 120px 56px;
}

.section-label {
  font-family: var(--mono);
  font-size: 10px;
  letter-spacing: 0.2em;
  text-transform: uppercase;
  color: var(--accent);
  margin-bottom: 20px;
  display: flex;
  align-items: center;
  gap: 12px;
  animation: glow-pulse 3s ease-in-out infinite;
}

@keyframes glow-pulse {
  0%, 100% {
    text-shadow: 0 0 10px rgba(179, 255, 87, 0.5);
  }
  50% {
    text-shadow: 0 0 20px rgba(179, 255, 87, 0.8);
  }
}

.section-label::after {
  content: "";
  flex: 0 0 40px;
  height: 1px;
  background: var(--accent);
  box-shadow: 0 0 8px var(--accent);
  animation: line-grow 2s ease-in-out infinite;
}

@keyframes line-grow {
  0%, 100% {
    width: 40px;
  }
  50% {
    width: 60px;
  }
}

.section h2 {
  font-family: var(--display);
  font-size: clamp(48px, 6vw, 88px);
  line-height: 0.95;
  letter-spacing: 0.04em;
  color: var(--text);
  max-width: 700px;
  margin-bottom: 48px;
}

.section h2 em {
  font-style: normal;
  color: var(--accent2);
  text-shadow: 0 0 40px var(--glow-cyan);
}

/* PILLARS */
.pillars {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 1px;
  background: var(--border);
  border: 1px solid var(--border);
  margin-top: 64px;
}

.pillar {
  background: var(--bg2);
  padding: 40px 36px;
  position: relative;
  overflow: hidden;
  transition: var(--transition-smooth);
  cursor: pointer;
}

.pillar::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 3px;
  background: linear-gradient(90deg, var(--accent), var(--accent2));
  transform: scaleX(0);
  transform-origin: left;
  transition: transform 0.5s cubic-bezier(0.4, 0, 0.2, 1);
}

.pillar::after {
  content: "";
  position: absolute;
  inset: 0;
  background: radial-gradient(circle at 50% 0%, rgba(179, 255, 87, 0.08), transparent 70%);
  opacity: 0;
  transition: opacity 0.5s;
}

.pillar:hover {
  background: var(--surface);
  transform: translateY(-4px);
  box-shadow: var(--shadow-xl), 0 0 40px rgba(179, 255, 87, 0.15);
}

.pillar:hover::before {
  transform: scaleX(1);
}

.pillar:hover::after {
  opacity: 1;
}

.pillar:hover .pillar-num {
  color: rgba(179, 255, 87, 0.15);
  transform: scale(1.05);
}

.pillar-num {
  font-family: var(--display);
  font-size: 64px;
  line-height: 1;
  color: rgba(245, 244, 255, 0.05);
  margin-bottom: 20px;
  letter-spacing: 0.04em;
  transition: var(--transition-smooth);
  position: relative;
  z-index: 1;
}

.pillar h3 {
  font-family: var(--sans);
  font-size: 18px;
  font-weight: 700;
  color: var(--text);
  margin-bottom: 12px;
  letter-spacing: -0.01em;
  position: relative;
  z-index: 1;
  transition: color 0.3s;
}

.pillar:hover h3 {
  color: var(--accent);
}

.pillar p {
  font-size: 14px;
  color: var(--muted);
  line-height: 1.7;
  position: relative;
  z-index: 1;
  transition: color 0.3s;
}

.pillar:hover p {
  color: var(--text);
}

/* ── STATS ── */
.stats-section {
  padding: 0 56px 120px;
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 1px;
  background: var(--border);
  border: 1px solid var(--border);
}

.stat-cell {
  background: var(--bg);
  padding: 48px 40px;
  display: flex;
  flex-direction: column;
  gap: 8px;
  position: relative;
  overflow: hidden;
  transition: var(--transition-smooth);
  cursor: default;
}

.stat-cell::before {
  content: "";
  position: absolute;
  inset: 0;
  background: radial-gradient(circle at center, rgba(179, 255, 87, 0.05), transparent 70%);
  opacity: 0;
  transition: opacity 0.4s;
}

.stat-cell::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 2px;
  background: linear-gradient(90deg, transparent, var(--accent), transparent);
  opacity: 0;
  transition: opacity 0.4s;
}

.stat-cell:hover {
  background: var(--bg2);
  transform: translateY(-4px);
  box-shadow: var(--shadow-lg);
}

.stat-cell:hover::before {
  opacity: 1;
}

.stat-cell:hover::after {
  opacity: 1;
}

.stat-cell:hover .stat-num {
  transform: scale(1.05);
  text-shadow: 0 0 30px rgba(179, 255, 87, 0.4);
}

.stat-num {
  font-family: var(--display);
  font-size: 56px;
  line-height: 1;
  letter-spacing: 0.04em;
  color: var(--text);
  transition: var(--transition-smooth);
  position: relative;
  z-index: 1;
}

.stat-num span {
  color: var(--accent);
  display: inline-block;
  transition: transform 0.3s;
}

.stat-cell:hover .stat-num span {
  transform: scale(1.1);
}

.stat-label {
  font-family: var(--mono);
  font-size: 11px;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  color: var(--muted);
  position: relative;
  z-index: 1;
  transition: color 0.3s;
}

.stat-cell:hover .stat-label {
  color: var(--accent);
}

/* ── FEATURED GAME ── */
.featured-section {
  padding: 0 56px 120px;
}

.featured-card {
  background: var(--surface);
  border: 1px solid var(--border);
  border-radius: 4px;
  overflow: hidden;
  display: grid;
  grid-template-columns: 1fr 1fr;
  min-height: 480px;
  position: relative;
  transition: var(--transition-smooth);
}

.featured-card:hover {
  border-color: rgba(179, 255, 87, 0.3);
  box-shadow: var(--shadow-xl), 0 0 60px rgba(179, 255, 87, 0.15);
  transform: translateY(-4px);
}

.featured-card::before {
  content: "";
  position: absolute;
  inset: 0;
  border-radius: 4px;
  border: 1px solid transparent;
  background: linear-gradient(
      135deg,
      rgba(179, 255, 87, 0.3),
      rgba(87, 255, 179, 0.2)
    )
    border-box;
  -webkit-mask:
    linear-gradient(#fff 0 0) padding-box,
    linear-gradient(#fff 0 0);
  -webkit-mask-composite: destination-out;
  mask:
    linear-gradient(#fff 0 0) padding-box,
    linear-gradient(#fff 0 0);
  mask-composite: exclude;
  pointer-events: none;
  opacity: 0.5;
  transition: opacity 0.4s;
}

.featured-card:hover::before {
  opacity: 1;
}

.featured-content {
  padding: 56px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  gap: 24px;
}

.featured-badge {
  font-family: var(--mono);
  font-size: 10px;
  letter-spacing: 0.15em;
  text-transform: uppercase;
  color: var(--accent3);
  display: flex;
  align-items: center;
  gap: 8px;
}

.featured-badge::before {
  content: "";
  width: 6px;
  height: 6px;
  border-radius: 50%;
  background: var(--accent3);
  box-shadow: 0 0 8px var(--accent3);
  animation: blink 2s ease-in-out infinite;
}

@keyframes blink {
  0%,
  100% {
    opacity: 1;
  }
  50% {
    opacity: 0.3;
  }
}

.featured-content h2 {
  font-family: var(--display);
  font-size: 56px;
  line-height: 0.95;
  letter-spacing: 0.04em;
  color: var(--text);
}

.featured-content p {
  font-size: 15px;
  color: var(--muted);
  line-height: 1.7;
  max-width: 380px;
}

.featured-content p strong {
  color: var(--text);
  font-weight: 500;
}

.featured-cta {
  display: inline-flex;
  align-items: center;
  gap: 12px;
  font-family: var(--mono);
  font-size: 12px;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  color: var(--bg);
  background: var(--accent2);
  padding: 16px 36px;
  border-radius: 2px;
  text-decoration: none;
  align-self: flex-start;
  transition:
    box-shadow 0.25s,
    transform 0.2s;
}

.featured-cta:hover {
  box-shadow: 0 0 40px var(--glow-cyan);
  transform: translateY(-2px);
}

.featured-visual {
  background: var(--bg2);
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  overflow: hidden;
}

/* Animated game art placeholder */
.game-art {
  width: 200px;
  height: 200px;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}

.game-art-ring {
  position: absolute;
  border-radius: 50%;
  border: 1px solid;
  animation: spin linear infinite;
}

.game-art-ring:nth-child(1) {
  width: 200px;
  height: 200px;
  border-color: rgba(255, 60, 172, 0.3);
  animation-duration: 12s;
}
.game-art-ring:nth-child(2) {
  width: 160px;
  height: 160px;
  border-color: rgba(0, 240, 255, 0.3);
  animation-duration: 8s;
  animation-direction: reverse;
}
.game-art-ring:nth-child(3) {
  width: 120px;
  height: 120px;
  border-color: rgba(255, 230, 0, 0.3);
  animation-duration: 6s;
}

.game-art-core {
  width: 72px;
  height: 72px;
  background: linear-gradient(135deg, var(--accent), var(--accent2));
  border-radius: 16px;
  box-shadow:
    0 0 40px var(--glow-pink),
    0 0 80px var(--glow-cyan);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32px;
  position: relative;
  z-index: 1;
  animation: pulse 3s ease-in-out infinite;
}

@keyframes spin {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}
@keyframes pulse {
  0%,
  100% {
    box-shadow:
      0 0 40px var(--glow-pink),
      0 0 80px var(--glow-cyan);
  }
  50% {
    box-shadow:
      0 0 60px var(--glow-pink),
      0 0 120px var(--glow-cyan);
  }
}

.featured-visual-label {
  position: absolute;
  bottom: 24px;
  font-family: var(--mono);
  font-size: 10px;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  color: rgba(245, 244, 255, 0.2);
}

/* ── FOOTER ── */
footer {
  border-top: 1px solid var(--border);
  padding: 48px 56px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.footer-logo {
  font-family: var(--display);
  font-size: 22px;
  letter-spacing: 0.12em;
  color: var(--text);
  text-decoration: none;
}

.footer-logo span {
  color: var(--accent);
}

.footer-links {
  display: flex;
  gap: 32px;
  list-style: none;
}

.footer-links a {
  font-family: var(--mono);
  font-size: 11px;
  letter-spacing: 0.08em;
  text-transform: uppercase;
  color: var(--muted);
  text-decoration: none;
  transition: color 0.2s;
  position: relative;
  padding-bottom: 2px;
}

.footer-links a::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  width: 0;
  height: 1px;
  background: var(--accent);
  transition: width 0.3s ease;
}

.footer-links a:hover {
  color: var(--text);
}

.footer-links a:hover::after {
  width: 100%;
}

.footer-copy {
  font-family: var(--mono);
  font-size: 11px;
  color: rgba(245, 244, 255, 0.2);
  letter-spacing: 0.06em;
}

/* ── ANIMATIONS ── */
@keyframes fadeUp {
  from {
    opacity: 0;
    transform: translateY(24px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

.reveal {
  opacity: 0;
  transform: translateY(32px);
  transition:
    opacity 0.7s ease,
    transform 0.7s ease;
}

.reveal.visible {
  opacity: 1;
  transform: translateY(0);
}

/* ── RESPONSIVE ── */
@media (max-width: 900px) {
  nav,
  .section,
  .featured-section,
  .stats-section,
  footer {
    padding-left: 24px;
    padding-right: 24px;
  }
  .hero {
    padding: 100px 24px 80px;
  }
  .scroll-hint {
    left: 24px;
  }
  .nav-links {
    display: none;
  }
  .pillars {
    grid-template-columns: 1fr;
  }
  .stats-section {
    grid-template-columns: 1fr 1fr;
  }
  .featured-card {
    grid-template-columns: 1fr;
  }
  .featured-visual {
    min-height: 280px;
  }
  footer {
    flex-direction: column;
    gap: 24px;
    text-align: center;
  }
  .footer-links {
    flex-wrap: wrap;
    justify-content: center;
  }
  body {
    cursor: auto;
  }
  .cursor,
  .cursor-ring {
    display: none;
  }

  /* Hide game thumbnails on mobile or make them smaller */
  .game-thumb {
    width: 80px;
    height: 80px;
    opacity: 0.4;
  }
  .thumb-placeholder {
    font-size: 32px;
  }
  .game-thumb-7,
  .game-thumb-8 {
    display: none;
  }
}

/* Enhanced link effects */
.nav-links a {
  font-family: var(--mono);
  font-size: 11px;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  color: var(--muted);
  text-decoration: none;
  transition: var(--transition-smooth);
  position: relative;
  padding-bottom: 2px;
}

.nav-links a::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  width: 0;
  height: 1px;
  background: var(--accent);
  transition: width 0.3s ease;
  box-shadow: 0 0 8px var(--accent);
}

.nav-links a:hover {
  color: var(--accent);
}

.nav-links a:hover::after {
  width: 100%;
}

/* Additional animations */
@keyframes shimmer {
  0% {
    background-position: -1000px 0;
  }
  100% {
    background-position: 1000px 0;
  }
}

.featured-cta,
.nav-cta,
.btn-primary,
.btn-secondary {
  position: relative;
  isolation: isolate;
}

/* Enhance hero text */
.hero h1 {
  animation: fadeUp 0.8s ease both;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
}

.hero h1 .outline {
  position: relative;
  display: inline-block;
  transition: var(--transition-smooth);
}

.hero h1 .outline:hover {
  transform: scale(1.02);
  filter: drop-shadow(0 0 20px rgba(179, 255, 87, 0.3));
}

.hero h1 .glow {
  position: relative;
  display: inline-block;
  transition: var(--transition-smooth);
}

.hero h1 .glow:hover {
  transform: scale(1.02);
  text-shadow: 
    0 0 30px var(--glow-cyan),
    0 0 60px var(--glow-cyan);
}

/* Enhance footer */
.footer-links a {
  position: relative;
  padding-bottom: 2px;
}

.footer-links a::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  width: 0;
  height: 1px;
  background: var(--accent);
  transition: width 0.3s ease;
}

.footer-links a:hover::after {
  width: 100%;
}

/* Add loading shimmer effect */
@keyframes loading-shimmer {
  0% {
    transform: translateX(-100%);
  }
  100% {
    transform: translateX(100%);
  }
}

/* Enhance orbs */
.orb-1 {
  width: 500px;
  height: 500px;
  background: radial-gradient(circle, rgba(179, 255, 87, 0.15), transparent 70%);
  top: 10%;
  left: -10%;
  animation: float-orb-1 20s ease-in-out infinite;
}

.orb-2 {
  width: 600px;
  height: 600px;
  background: radial-gradient(circle, rgba(87, 255, 179, 0.12), transparent 70%);
  bottom: 10%;
  right: -15%;
  animation: float-orb-2 25s ease-in-out infinite;
}

@keyframes float-orb-1 {
  0%, 100% {
    transform: translate(0, 0) scale(1);
  }
  50% {
    transform: translate(50px, -50px) scale(1.1);
  }
}

@keyframes float-orb-2 {
  0%, 100% {
    transform: translate(0, 0) scale(1);
  }
  50% {
    transform: translate(-50px, 50px) scale(1.15);
  }
}

/* Smooth scrollbar */
::-webkit-scrollbar {
  width: 12px;
}

::-webkit-scrollbar-track {
  background: var(--bg);
  border-left: 1px solid var(--border);
}

::-webkit-scrollbar-thumb {
  background: linear-gradient(180deg, var(--accent), var(--accent2));
  border-radius: 6px;
  border: 2px solid var(--bg);
}

::-webkit-scrollbar-thumb:hover {
  background: var(--accent);
  box-shadow: 0 0 10px var(--accent);
}

/* Selection styling */
::selection {
  background: var(--accent);
  color: var(--bg);
}

::-moz-selection {
  background: var(--accent);
  color: var(--bg);
}
